<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agenda">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_laucher_agenda"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_laucher_agenda_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight">
        <activity android:name=".ConfirmaExclusao" />
        <activity android:name=".Despedida" />
        <activity android:name=".CadastroUsuario" />
        <activity android:name=".CadastroContato" />
        <activity android:name=".AtualizaContato" />
        <activity android:name=".Contatos" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>